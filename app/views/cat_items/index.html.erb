<p id="notice"><%= notice %></p>

<h1>Cat Items</h1>

<table>
  <thead>
    <tr>
      <th>Cat</th>
      <th>Cart</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cat_items.each do |cat_item| %>
      <tr>
        <td><%= cat_item.cat %></td>
        <td><%= cat_item.cart %></td>
        <td><%= link_to 'Show', cat_item %></td>
        <td><%= link_to 'Edit', edit_cat_item_path(cat_item) %></td>
        <td><%= link_to 'Destroy', cat_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cat Item', new_cat_item_path %>
